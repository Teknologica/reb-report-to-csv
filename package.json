  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.4.1",
    "clear": "^0.1.0",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "inquirer": "^6.1.0",
    "multispinner": "^0.2.1",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-destructuring": "^7.0.0-beta.56",
